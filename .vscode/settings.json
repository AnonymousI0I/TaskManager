{
  "java.project.referencedLibraries": [
    "${workspaceFolder}/javafx-sdk-21.0.2/lib/*.jar"
  ],

  "java.debug.settings.vmArgs":
    "-XstartOnFirstThread --enable-native-access=ALL-UNNAMED -Dprism.order=sw --module-path javafx-sdk-21.0.2/lib --add-modules javafx.controls,javafx.fxml",

  "java.debug.settings.runArgs": [
    "-XstartOnFirstThread",
    "--enable-native-access=ALL-UNNAMED",
    "-Dprism.order=sw",
    "--module-path", "javafx-sdk-21.0.2/lib",
    "--add-modules", "javafx.controls,javafx.fxml"
  ]
}
